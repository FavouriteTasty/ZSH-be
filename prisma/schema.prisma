datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

enum UserType {
  VIEW
  NORMAL
  ADMIN
}

model User {
  id Int @id @default(autoincrement())
  username String
  password String
  type UserType
}

enum HistoryType {
  PAST_HISTORY
  SURGERY_HISTORY
  ALLERGIC_HISTORY
  VACCINATION_HISTORY
  IMPORTANT_DRUG_HISTORY
  BLOOD_TRANSFUSION_HISTORY
  SMOKING_HISTORY
  DRINKING_HISTORY
  MENSTRUAL_HISTORY
  MARITAL_HISTORY
  FAMILY_HISTORY

  @@map("history_types")
}

model UserProfile {
  id             String              @id
  name           String?
  sex            String?
  ethnicity      String?
  age            Int?
  birth          String?
  country        String?
  job            String?
  maritalStatus  String?
  nativePlace    String?
  address        String?
  phone          String?
  contact        String?
  relation       String?
  contactPhone   String?
  avatar         String?
  medicalHistory MedicalHistory[] @relation
  hospitalization Hospitalization? @relation
  stentPlacement StentPlacement? @relation
  preExamination PreoperativeExaminationForStentRemoval? @relation
  stentRemoval StentRemoval? @relation
  followup Followup? @relation
}

model MedicalHistory {
  id            Int         @id @default(autoincrement())
  userProfileId String        
  userProfile   UserProfile @relation(fields: [userProfileId], references: [id])
  description String?
  date        String?
  type        HistoryType?
  @@unique([userProfileId, date, description], name: "history_unique")
  @@map("medical_histories")
}

